{% extends "base.html" %}

{% block stylesheet %}
<style>
    body {
        font-family: Arial, sans-serif;
        background-color: #f5f5f5;
        margin: 0;
        padding: 0;
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100vh;
    }

    .container {
        background: #fff;
        border-radius: 10px;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        padding: 20px;
        max-width: 600px;
        width: 90%;
        text-align: center;
    }

    h1 {
        color: #333;
        margin-top: 20px;
    }

    p {
        color: #666;
    }
</style>
{% endblock %}


